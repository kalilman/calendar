// Prisma schema placeholder

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  events Event[]
}

model Event {
  id       Int      @id @default(autoincrement())
  title    String
  start    DateTime
  end      DateTime
  user     User     @relation(fields: [userId], references: [id])
  userId   Int
}
